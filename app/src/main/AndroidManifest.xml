<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marcos.androidexamples">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".app.activity.ZoomableImageActivity"
            android:label="@string/zoomable_imageview"
            android:parentActivityName=".app.activity.GesturesActivity" />
        <activity
            android:name=".app.activity.SearchViewActivity"
            android:label="@string/search_view"
            android:parentActivityName=".app.activity.ToolbarActivity" />
        <activity
            android:name=".app.activity.ToolbarWithTabsActivity"
            android:label="@string/toolbar_tabs"
            android:parentActivityName=".app.activity.ToolbarActivity" />
        <activity
            android:name=".app.activity.XmlSelectorsActivity"
            android:label="@string/xml_selectors"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.AnotherViewsActivity"
            android:label="@string/another_views"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.GridViewActivity"
            android:label="@string/gridview"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.ViewPagerActivity"
            android:label="@string/view_pager"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.ViewPagerTitleActivity"
            android:label="@string/view_pager_with_title"
            android:parentActivityName=".app.activity.ViewPagerActivity" />
        <activity
            android:name=".app.activity.SimpleViewPagerActivity"
            android:label="@string/simple_view_pager"
            android:parentActivityName=".app.activity.ViewPagerActivity" />
        <activity
            android:name=".app.activity.IntroSliderActivity"
            android:label="@string/intro_slider"
            android:parentActivityName=".app.activity.ViewPagerActivity" />
        <activity
            android:name=".app.activity.ImageSwitcherActivity"
            android:label="@string/image_switcher"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.WebViewActivity"
            android:label="@string/web_view"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.ToolbarActivity"
            android:label="@string/toolbar"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.GesturesActivity"
            android:label="@string/gestures"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.DraggableViewActivity"
            android:label="@string/draggable_view"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.DrawingShapesActivity"
            android:label="@string/drawing_shapes"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.FragmentsActivity"
            android:label="@string/fragments"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.RecyclerViewActivity"
            android:label="@string/recycler_view"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.SpinnerCompleteCheckActivity"
            android:label="@string/spinner_complete_check"
            android:parentActivityName=".app.activity.AnotherViewsActivity" />
        <activity
            android:name=".app.activity.SaveInstanceStateActivity"
            android:label="@string/save_instance_state"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity
            android:name=".app.activity.AnimationsActivity"
            android:label="@string/animations"
            android:parentActivityName=".app.activity.MainActivity" />
        <activity android:name=".app.activity.AnimationDrawableActivity"
            android:label="@string/animation_drawable"
            android:parentActivityName=".app.activity.AnimationsActivity"/>
    </application>

</manifest>